{
	"name": "blog-template",
	"type": "module",
	"version": "1.1.0",
	"description": "A template for creating a blog with Astro",
	"author": "danielcgilibert",
	"license": "GPL-3.0-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielcgilibert/blog-template"
	},
	"keywords": [
		"astro",
		"astrojs",
		"astro-template",
		"astro-blog",
		"astro-b"
	],
	"bugs": {
		"url": "https://github.com/danielcgilibert/blog-template/issues"
	},
	"scripts": {
		"dev": "npx tinacms dev -c \"astro dev\"",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/react": "^3.6.2",
		"@astrojs/rss": "4.0.1",
		"astro": "4.0.8",
		"astro-font": "^0.0.72",
		"astro-icon": "^1.1.0",
		"dotenv": "^16.4.5",
		"react": "^18.3.1",
		"react-cookie-consent": "^9.0.0",
		"react-dom": "^18.3.1",
		"remark-gemoji": "^8.0.0",
		"vanilla-cookieconsent": "3.0.1",
		"vite-plugin-virtual": "^0.3.0"
	},
	"devDependencies": {
		"@astrojs/mdx": "2.0.3",
		"@astrojs/sitemap": "3.0.4",
		"@astrojs/tailwind": "5.1.0",
		"@pagefind/default-ui": "^1.0.4",
		"@tailwindcss/typography": "0.5.10",
		"@tinacms/cli": "^1.5.31",
		"@typescript-eslint/parser": "^6.16.0",
		"clsx": "2.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-astro": "^0.31.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.16.4",
		"pagefind": "^1.0.3",
		"prettier": "^3.0.3",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.12.0",
		"reading-time": "^1.5.0",
		"shiki": "^1.16.2",
		"slugify": "^1.6.6",
		"tailwind-merge": "2.0.0",
		"tailwindcss": "3.3.5",
		"tinacms": "^1.5.22",
		"typescript": "^5.2.2"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=."
		]
	},
	"packageManager": "pnpm@9.1.2+sha1.907f57a52517de34399d66401bf220a549e47b57"
}
