---
import type { Icon } from "~/lib/types";

type Props = Icon;

const {
  width = "20",
  height = "20",
  color = "currentColor",
  strokeWidth = "1.5",
  size,
} = Astro.props;
---

<svg
xmlns="http://www.w3.org/2000/svg"
width={size ? size : width}
height={size ? size : height}
fill="none"
viewBox="0 0 24 24"
stroke={color}
>
<path
  stroke-linecap="round"
  stroke-linejoin="round"
  stroke-width={strokeWidth}
  d="M4 6h16M4 12h16M4 18h7"></path>
</svg>